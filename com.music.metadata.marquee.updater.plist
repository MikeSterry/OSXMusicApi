<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.music.metadata.marquee.updater.plist</string>
        <key>ProgramArguments</key>
        <array>
            <string>.venv/bin/python3</string>
            <string>garage-mini-marquee-updater.py</string>
        </array>
        <key>KeepAlive</key>
        <true/>
        <key>WorkingDirectory</key>
        <string>/Users/YourUserName/bin/OSXMusicApi/</string>
        <key>StandardErrorPath</key>
        <string>/tmp/local.job.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/local.job.out</string>
        <key>RunAtLoad</key>
        <true/>
    </dict>
</plist>
